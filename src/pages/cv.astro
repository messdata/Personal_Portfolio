---
import Particles from "../components/welcome/Particles";
---

<!doctype html>
<html lang="en" style="margin: 0; padding: 0; height: 100%; overflow: hidden;">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Resume - Chinmay Patil</title>
    <style is:global>
      * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
      }

      html,
      body {
        margin: 0 !important;
        padding: 0 !important;
        width: 100%;
        height: 100%;
        overflow: hidden;
        background: #0a0a0a;
        font-family:
          system-ui,
          -apple-system,
          sans-serif;
      }

      /* Floating Navigation */
      .floating-nav {
        position: fixed;
        top: 1rem;
        left: 50%;
        transform: translateX(-50%);
        z-index: 1000;
        display: flex;
        align-items: center;
        gap: 0.5rem;
        padding: 0.25rem 0.7rem;
        background: rgba(0, 0, 0, 0.7);
        backdrop-filter: blur(10px);
        border: 1px solid rgba(255, 255, 255, 0.1);
        border-radius: 2rem;
        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.5);
      }

      .nav-link {
        color: rgba(255, 255, 255, 0.8);
        text-decoration: none;
        padding: 0.5rem 1rem;
        border-radius: 1.5rem;
        transition: all 0.2s ease;
        font-size: 0.875rem;
        font-weight: 500;
      }

      .nav-link:hover {
        background: rgba(255, 255, 255, 0.1);
        color: white;
      }

      /* Main Container */
      .resume-container {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100vh;
        overflow-y: auto;
        padding: 4rem 1rem 2rem;
      }

      .resume-wrapper {
        max-width: 1200px;
        margin: 0 auto;
      }

      /* Header Section */
      .resume-header {
        text-align: center;
        margin-bottom: 1.125rem;
      }

      .resume-title {
        font-size: 2.5rem;
        font-weight: 700;
        color: white;
        margin-bottom: 0.5rem;
      }

      @media (min-width: 768px) {
        .resume-title {
          font-size: 3.5rem;
        }
      }

      .resume-subtitle {
        font-size: 1rem;
        color: rgba(255, 255, 255, 0.6);
        margin-bottom: 2rem;
        text-transform: uppercase;
        letter-spacing: 0.15em;
      }

      /* Download Buttons */
      .download-buttons {
        display: flex;
        flex-direction: column;
        gap: 1rem;
        align-items: center;
        margin-bottom: 3rem;
      }

      @media (min-width: 640px) {
        .download-buttons {
          flex-direction: row;
          justify-content: center;
        }
      }

      .download-btn {
        display: flex;
        align-items: center;
        gap: 0.75rem;
        padding: 1rem 2rem;
        background: linear-gradient(135deg, #1a8fb6, #0df4f4);
        border: none;
        border-radius: 0.5rem;
        color: #000;
        font-size: 0.875rem;
        font-weight: 600;
        text-decoration: none;
        cursor: pointer;
        transition: all 0.3s ease;
        min-width: 200px;
        justify-content: center;
      }

      .download-btn:hover {
        transform: translateY(-2px);
        box-shadow: 0 20px 25px -5px rgba(26, 143, 182, 0.3);
      }

      .download-btn.secondary {
        background: linear-gradient(135deg, #ff00ff, #ff66ff);
      }

      .download-btn svg {
        width: 1.25rem;
        height: 1.25rem;
      }

      /* Quick Links */
      .quick-links {
        display: flex;
        justify-content: center;
        gap: 1.5rem;
        margin-bottom: 3rem;
      }

      .quick-link {
        display: flex;
        align-items: center;
        gap: 0.5rem;
        padding: 0.75rem 1.5rem;
        background: rgba(255, 255, 255, 0.05);
        border: 1px solid rgba(255, 255, 255, 0.1);
        border-radius: 2rem;
        color: rgba(255, 255, 255, 0.8);
        text-decoration: none;
        font-size: 0.875rem;
        transition: all 0.3s ease;
      }

      .quick-link:hover {
        background: rgba(255, 255, 255, 0.1);
        border-color: rgba(0, 255, 255, 0.3);
        color: #00ffff;
        transform: translateY(-2px);
      }

      .quick-link svg {
        width: 1rem;
        height: 1rem;
      }

      /* Tab Navigation */
      .tab-nav {
        display: flex;
        justify-content: center;
        gap: 0.5rem;
        margin-bottom: 2rem;
        flex-wrap: wrap;
        padding: 0 1rem;
      }

      .tab-button {
        padding: 0.75rem 1.5rem;
        background: rgba(255, 255, 255, 0.05);
        border: 1px solid rgba(255, 255, 255, 0.1);
        border-radius: 2rem;
        color: rgba(255, 255, 255, 0.6);
        font-size: 0.875rem;
        font-weight: 500;
        cursor: pointer;
        transition: all 0.3s ease;
        white-space: nowrap;
      }

      .tab-button:hover {
        background: rgba(255, 255, 255, 0.1);
        color: rgba(255, 255, 255, 0.9);
      }

      .tab-button.active {
        background: linear-gradient(
          135deg,
          rgba(26, 143, 182, 0.2),
          rgba(13, 244, 244, 0.2)
        );
        border-color: rgba(0, 255, 255, 0.3);
        color: #00ffff;
        box-shadow: 0 0 20px rgba(0, 255, 255, 0.2);
      }

      /* Tab Content */
      .tab-content {
        display: none;
        animation: fadeIn 0.5s ease;
      }

      .tab-content.active {
        display: block;
      }

      @keyframes fadeIn {
        from {
          opacity: 0;
          transform: translateY(10px);
        }
        to {
          opacity: 1;
          transform: translateY(0);
        }
      }

      /* Content Cards */
      .content-card {
        background: rgba(0, 0, 0, 0.5);
        border: 1px solid rgba(255, 255, 255, 0.1);
        border-radius: 0.75rem;
        padding: 1.1rem;
        margin-bottom: 1.5rem;
        backdrop-filter: blur(10px);
        transition: all 0.3s ease;
      }

      .content-card:hover {
        border-color: rgba(0, 255, 255, 0.3);
        box-shadow: 0 10px 30px rgba(0, 255, 255, 0.1);
        transform: translateY(-2px);
      }

      /* Profile Section */
      .profile-grid {
        display: grid;
        grid-template-columns: 1fr;
        gap: 1.5rem;
      }

      @media (min-width: 768px) {
        .profile-grid {
          grid-template-columns: 1fr 1fr;
        }
      }

      .profile-text {
        font-size: 1rem;
        line-height: 1.8;
        color: rgba(255, 255, 255, 0.8);
      }

      .profile-stats {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
        gap: 1rem;
      }

      .stat-item {
        text-align: center;
        padding: 0.5rem;
        background: rgba(255, 255, 255, 0.05);
        border-radius: 0.5rem;
        border: 1px solid rgba(255, 255, 255, 0.1);
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
      }

      .stat-value {
        font-size: 1.5rem;
        font-weight: 700;
        color: #00ffff;
        margin-bottom: 0.25rem;
      }

      .stat-label {
        font-size: 0.75rem;
        color: rgba(255, 255, 255, 0.5);
        text-transform: uppercase;
        letter-spacing: 0.1em;
      }

      /* Experience Timeline */
      .timeline {
        position: relative;
        padding-left: 2rem;
      }

      .timeline::before {
        content: "";
        position: absolute;
        left: 0;
        top: 0;
        bottom: 0;
        width: 2px;
        background: linear-gradient(to bottom, #1a8fb6, #0df4f4, #ff00ff);
      }

      .timeline-item {
        position: relative;
        margin-bottom: 2rem;
      }

      .timeline-dot {
        position: absolute;
        left: -2.5rem;
        top: 0.5rem;
        width: 1rem;
        height: 1rem;
        border-radius: 50%;
        background: #00ffff;
        box-shadow: 0 0 10px rgba(0, 255, 255, 0.5);
        z-index: 10;
      }

      .timeline-dot::before {
        content: "";
        position: absolute;
        inset: -4px;
        border-radius: 50%;
        border: 2px solid rgba(0, 255, 255, 0.3);
        animation: pulse 2s ease-in-out infinite;
      }

      @keyframes pulse {
        0%,
        100% {
          transform: scale(1);
          opacity: 1;
        }
        50% {
          transform: scale(1.2);
          opacity: 0.5;
        }
      }

      .experience-card {
        background: rgba(0, 0, 0, 0.3);
        border: 1px solid rgba(255, 255, 255, 0.1);
        border-radius: 0.5rem;
        padding: 1.5rem;
        transition: all 0.3s ease;
      }

      .experience-card:hover {
        background: rgba(0, 0, 0, 0.5);
        border-color: rgba(0, 255, 255, 0.3);
      }

      .experience-header {
        display: flex;
        justify-content: space-between;
        align-items: flex-start;
        margin-bottom: 1rem;
        flex-wrap: wrap;
        gap: 0.5rem;
      }

      .job-title {
        font-size: 1.125rem;
        font-weight: 600;
        color: #00ffff;
      }

      .company-name {
        font-size: 0.875rem;
        color: rgba(255, 255, 255, 0.7);
        margin-top: 0.25rem;
      }

      .job-duration {
        font-size: 0.75rem;
        color: rgba(255, 255, 255, 0.5);
        background: rgba(255, 255, 255, 0.05);
        padding: 0.25rem 0.75rem;
        border-radius: 1rem;
      }

      .job-description {
        font-size: 0.875rem;
        line-height: 1.6;
        color: rgba(255, 255, 255, 0.7);
      }

      .job-description ul {
        list-style: none;
        padding: 0;
        margin-top: 1rem;
      }

      .job-description li {
        position: relative;
        padding-left: 1.5rem;
        margin-bottom: 0.5rem;
      }

      .job-description li::before {
        content: "▹";
        position: absolute;
        left: 0;
        color: #00ffff;
        font-weight: bold;
      }

      /* Skills Grid */
      .skills-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
        gap: 1rem;
      }

      .skill-category {
        background: rgba(0, 0, 0, 0.3);
        border: 1px solid rgba(255, 255, 255, 0.1);
        border-radius: 0.5rem;
        padding: 1.5rem;
        transition: all 0.3s ease;
      }

      .skill-category:hover {
        border-color: rgba(255, 0, 255, 0.3);
        box-shadow: 0 10px 30px rgba(255, 0, 255, 0.1);
        transform: translateY(-4px);
      }

      .skill-category-title {
        font-size: 0.875rem;
        font-weight: 600;
        color: #ff00ff;
        text-transform: uppercase;
        letter-spacing: 0.1em;
        margin-bottom: 1rem;
        display: flex;
        align-items: center;
        gap: 0.5rem;
      }

      .skill-category-icon {
        width: 1.25rem;
        height: 1.25rem;
        color: #ff00ff;
      }

      .skill-tags {
        display: flex;
        flex-wrap: wrap;
        gap: 0.5rem;
      }

      .skill-tag {
        padding: 0.5rem 1rem;
        background: rgba(255, 255, 255, 0.05);
        border: 1px solid rgba(255, 255, 255, 0.1);
        border-radius: 1.5rem;
        font-size: 0.75rem;
        color: rgba(255, 255, 255, 0.8);
        transition: all 0.3s ease;
      }

      .skill-tag:hover {
        background: rgba(255, 0, 255, 0.1);
        border-color: rgba(255, 0, 255, 0.3);
        color: #ff00ff;
      }

      /* Education Cards */
      .education-grid {
        display: grid;
        grid-template-columns: 1fr;
        gap: 1.5rem;
      }

      @media (min-width: 768px) {
        .education-grid {
          grid-template-columns: repeat(2, 1fr);
        }
      }

      .education-card {
        background: rgba(0, 0, 0, 0.3);
        border: 1px solid rgba(255, 255, 255, 0.1);
        border-radius: 0.5rem;
        padding: 1.5rem;
        transition: all 0.3s ease;
        position: relative;
        overflow: hidden;
      }

      .education-card::before {
        content: "";
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        height: 3px;
        background: linear-gradient(90deg, #ffff00, #00ff00);
      }

      .education-card:hover {
        background: rgba(0, 0, 0, 0.5);
        border-color: rgba(255, 255, 0, 0.3);
        transform: translateY(-4px);
      }

      .degree {
        font-size: 1.125rem;
        font-weight: 600;
        color: #ffff00;
        margin-bottom: 0.5rem;
      }

      .university {
        font-size: 0.875rem;
        color: rgba(255, 255, 255, 0.7);
        margin-bottom: 0.5rem;
      }

      .education-duration {
        font-size: 0.75rem;
        color: rgba(255, 255, 255, 0.5);
        margin-bottom: 1rem;
      }

      .education-details {
        font-size: 0.875rem;
        line-height: 1.6;
        color: rgba(255, 255, 255, 0.7);
      }

      .education-details ul {
        list-style: none;
        padding: 0;
        margin-top: 0.5rem;
      }

      .education-details li {
        padding-left: 1rem;
        margin-bottom: 0.25rem;
        position: relative;
      }

      .education-details li::before {
        content: "•";
        position: absolute;
        left: 0;
        color: #ffff00;
      }

      /* Contact Info */
      .contact-info {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
        gap: 1rem;
      }

      .contact-item {
        display: flex;
        align-items: center;
        gap: 1rem;
        padding: 1rem;
        background: rgba(255, 255, 255, 0.05);
        border: 1px solid rgba(255, 255, 255, 0.1);
        border-radius: 0.5rem;
        transition: all 0.3s ease;
        margin-bottom: 1rem;
      }

      .contact-item:hover {
        background: rgba(255, 255, 255, 0.1);
        border-color: rgba(0, 255, 0, 0.3);
      }

      .contact-icon {
        width: 2.5rem;
        height: 2.5rem;
        border-radius: 50%;
        background: linear-gradient(
          135deg,
          rgba(0, 255, 0, 0.2),
          rgba(0, 255, 255, 0.2)
        );
        display: flex;
        align-items: center;
        justify-content: center;
        flex-shrink: 0;
      }

      .contact-icon svg {
        width: 1.25rem;
        height: 1.25rem;
        color: #00ff00;
      }

      .contact-details {
        flex: 1;
      }

      .contact-label {
        font-size: 0.75rem;
        color: rgba(255, 255, 255, 0.5);
        text-transform: uppercase;
        letter-spacing: 0.1em;
      }

      .contact-value {
        font-size: 0.875rem;
        color: rgba(255, 255, 255, 0.9);
        margin-top: 0.25rem;
      }

      .contact-value a {
        color: inherit;
        text-decoration: none;
        transition: color 0.3s ease;
      }

      .contact-value a:hover {
        color: #00ff00;
      }

      /* Mobile Adjustments - Tablet */
      @media (max-width: 768px) {
        .floating-nav {
          padding: 0.5rem 0.75rem;
          gap: 0.25rem;
          width: calc(100% - 2rem);
          max-width: 420px;
          justify-content: center;
        }

        .nav-link {
          font-size: 0.75rem;
          padding: 0.4rem 0.7rem;
        }

        .resume-container {
          padding: 4.5rem 1rem 2rem;
        }

        .resume-title {
          font-size: 2rem;
        }

        .resume-subtitle {
          font-size: 0.875rem;
          margin-bottom: 1.5rem;
        }

        .download-buttons {
          gap: 0.75rem;
          margin-bottom: 2rem;
        }

        .download-btn {
          padding: 0.85rem 1.5rem;
          font-size: 0.8rem;
          min-width: 180px;
        }

        .quick-links {
          flex-wrap: wrap;
          gap: 0.75rem;
          margin-bottom: 2rem;
        }

        .quick-link {
          padding: 0.6rem 1rem;
          font-size: 0.8rem;
        }

        .tab-nav {
          gap: 0.4rem;
          margin-bottom: 1.5rem;
          padding: 0 0.5rem;
        }

        .tab-button {
          padding: 0.6rem 1rem;
          font-size: 0.75rem;
        }

        .timeline {
          padding-left: 1.5rem;
        }

        .timeline-dot {
          left: -2rem;
          width: 0.875rem;
          height: 0.875rem;
        }

        .content-card {
          padding: 1.25rem;
          margin-bottom: 1.25rem;
        }

        .experience-card {
          padding: 1.25rem;
        }

        .skill-category {
          padding: 1.25rem;
        }

        .education-card {
          padding: 1.25rem;
        }
      }

      /* Mobile Adjustments - Small screens */
      @media (max-width: 480px) {
        .floating-nav {
          padding: 0.4rem 0.5rem;
          gap: 0.15rem;
          width: calc(100% - 1.5rem);
        }

        .nav-link {
          font-size: 0.65rem;
          padding: 0.35rem 0.5rem;
        }

        .resume-container {
          padding: 4.25rem 0.75rem 1.5rem;
        }

        .resume-title {
          font-size: 1.5rem;
        }

        .resume-subtitle {
          font-size: 0.7rem;
          margin-bottom: 1.25rem;
          letter-spacing: 0.1em;
        }

        .download-buttons {
          gap: 0.6rem;
          margin-bottom: 1.25rem;
        }

        .download-btn {
          padding: 0.7rem 1rem;
          font-size: 0.7rem;
          min-width: 150px;
          gap: 0.5rem;
        }

        .download-btn svg {
          width: 1rem;
          height: 1rem;
        }

        .quick-links {
          gap: 0.5rem;
          margin-bottom: 1.25rem;
        }

        .quick-link {
          padding: 0.5rem 0.8rem;
          font-size: 0.7rem;
          gap: 0.35rem;
        }

        .quick-link svg {
          width: 0.85rem;
          height: 0.85rem;
        }

        .tab-nav {
          gap: 0.3rem;
          margin-bottom: 1rem;
          padding: 0 0.25rem;
        }

        .tab-button {
          padding: 0.45rem 0.65rem;
          font-size: 0.6rem;
        }

        .content-card {
          padding: 1rem;
          margin-bottom: 1rem;
        }

        .profile-grid {
          gap: 1rem;
        }

        .profile-text {
          font-size: 0.8rem;
          line-height: 1.6;
        }

        .profile-stats {
          gap: 0.5rem;
        }

        .stat-item {
          padding: 0.4rem;
        }

        .stat-value {
          font-size: 1.125rem;
        }

        .stat-label {
          font-size: 0.55rem;
          letter-spacing: 0.05em;
        }

        .timeline {
          padding-left: 1.25rem;
        }

        .timeline-item {
          margin-bottom: 1.25rem;
        }

        .timeline-dot {
          left: -1.65rem;
          width: 0.75rem;
          height: 0.75rem;
        }

        .experience-card {
          padding: 1rem;
        }

        .job-title {
          font-size: 0.9rem;
        }

        .company-name {
          font-size: 0.7rem;
        }

        .job-duration {
          font-size: 0.6rem;
          padding: 0.2rem 0.5rem;
        }

        .job-description {
          font-size: 0.75rem;
          line-height: 1.5;
        }

        .job-description ul {
          margin-top: 0.75rem;
        }

        .job-description li {
          padding-left: 1rem;
          margin-bottom: 0.4rem;
        }

        .skills-grid {
          grid-template-columns: 1fr;
          gap: 0.75rem;
        }

        .skill-category {
          padding: 1rem;
        }

        .skill-category-title {
          font-size: 0.7rem;
          margin-bottom: 0.75rem;
        }

        .skill-category-icon {
          width: 1rem;
          height: 1rem;
        }

        .skill-tags {
          gap: 0.35rem;
        }

        .skill-tag {
          padding: 0.35rem 0.65rem;
          font-size: 0.6rem;
        }

        .education-grid {
          gap: 0.75rem;
        }

        .education-card {
          padding: 1rem;
        }

        .degree {
          font-size: 0.9rem;
        }

        .university {
          font-size: 0.7rem;
        }

        .education-duration {
          font-size: 0.6rem;
          margin-bottom: 0.75rem;
        }

        .education-details {
          font-size: 0.75rem;
          line-height: 1.5;
        }

        .contact-info {
          grid-template-columns: 1fr;
          gap: 0.6rem;
        }

        .contact-item {
          padding: 0.75rem;
          gap: 0.75rem;
        }

        .contact-icon {
          width: 2rem;
          height: 2rem;
        }

        .contact-icon svg {
          width: 1rem;
          height: 1rem;
        }

        .contact-label {
          font-size: 0.6rem;
        }

        .contact-value {
          font-size: 0.75rem;
          word-break: break-word;
        }
      }
    </style>
  </head>
  <body>
    <!-- Particle Background -->
    <div
      style="position: fixed; top: 0; left: 0; right: 0; bottom: 0; z-index: 0;"
    >
      <Particles
        client:load
        particleColors={["#1a8fb6", "#ff00ff", "#ffff00", "#00ff00"]}
        particleCount={200}
        particleSpread={20}
        speed={0.1}
        particleBaseSize={60}
        moveParticlesOnHover={true}
        particleHoverFactor={1.3}
        alphaParticles={true}
        disableRotation={false}
        sizeRandomness={1.5}
      />
    </div>

    <!-- Floating Navigation -->
    <nav class="floating-nav">
      <a
        href="/"
        class="nav-link"
        onclick="sessionStorage.setItem('skipIntro', 'true')">Home</a
      >
      <a href="/about" class="nav-link">About</a>
      <a href="/project" class="nav-link">Projects</a>
      <a href="/contact" class="nav-link">Contact</a>
    </nav>

    <!-- Main Content -->
    <main class="resume-container">
      <div class="resume-wrapper">
        <!-- Header -->
        <header class="resume-header">
          <h1 class="resume-title">Chinmay Patil</h1>
          <p class="resume-subtitle">Business Analytics Professional</p>

          <!-- Download Buttons -->
